{
  "name": "middy-lesslog",
  "version": "2.0.3",
  "description": "Middy middleware for `lesslog`, a teeny-tiny and severless-ready logging utility.",
  "author": "Robert Da Silva <mail@robdasilva.com> (https://robdasilva.com)",
  "homepage": "https://github.com/robdasilva/middy-lesslog#readme",
  "license": "MIT",
  "keywords": [
    "aws",
    "cloudwatch",
    "debug",
    "debugger",
    "lambda",
    "log",
    "logger",
    "logless",
    "metrics",
    "middy",
    "monitoring",
    "serverless"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "files": [
    "index.d.ts",
    "index.js",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "tsc && pnpm run format index.d.ts index.js",
    "format": "prettier --write",
    "format:all": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint --ext .js,.ts --fix --ignore-path .gitignore",
    "lint:all": "pnpm run lint .",
    "lint:check": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "prepare": "lefthook install && pnpm run preversion && pnpm run build",
    "preversion": "pnpm run lint:all && pnpm run test:all",
    "postversion": "git push && git push --tags",
    "test": "jest --bail --find-related-tests",
    "test:all": "jest --pass-with-no-tests",
    "test:ci": "jest --ci --coverage --pass-with-no-tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/robdasilva/middy-lesslog.git"
  },
  "bugs": {
    "url": "https://github.com/robdasilva/middy-lesslog/issues"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@8.15.5+sha256.4b4efa12490e5055d59b9b9fc9438b7d581a6b7af3b5675eb5c5f447cee1a589",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.81",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "lefthook": "^1.6.7",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "@middy/core": "^2.4.3",
    "lesslog": "^0.2.3"
  }
}
